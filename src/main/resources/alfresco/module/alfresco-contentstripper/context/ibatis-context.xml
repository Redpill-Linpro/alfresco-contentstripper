<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>

	<bean id="keystoreFileLocationSqlSessionFactory" class="org.alfresco.ibatis.HierarchicalSqlSessionFactoryBean">
		<property name="resourceLoader" ref="dialectResourceLoader" />
		<property name="dataSource" ref="dataSource" />
		<property name="configLocation">
			<value>classpath:alfresco/module/${project.artifactId}/ibatis/keystoreFileLocation-SqlMapConfig.xml</value>
		</property>
	</bean>
   
    <bean id="keystoreFileLocationSqlSessionTemplate" class="org.mybatis.spring.SqlSessionTemplate">
        <constructor-arg index="0" ref="keystoreFileLocationSqlSessionFactory"/>
    </bean>
    
    <bean id="keystoreFileLocationDAO" class="org.redpill.alfresco.repo.domain.KeystoreFileLocationDAOImpl">
    	<property name="template" ref="keystoreFileLocationSqlSessionTemplate"/>
    </bean>
</beans>